#pragma once

{%- import "common.hpp.in" as common %}

#include "aliases.hpp"

namespace gir {
namespace {{ enum_.namespace.name }} {

class {{ enum_.name }}
{
public:
    enum
    {
        {%- for k,v in enum_.members %}
        {{ k }} = {{ v }},
        {%- endfor %}
    };

    {% for m in enum_.methods.values() %}
    {{ common.method(m) }}
    {% endfor %}
};

} //namespace {{ enum_.namespace.name }};
} //namespace gir;
